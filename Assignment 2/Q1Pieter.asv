s = tf('s');

Wi1=((s/(16*pi))+0.3)/((s/(64*pi))+1);
Wi2 = Wi1;
Wi=[Wi1 0;0 Wi2];

Wo1=((0.05*s)+0.02)/((0.01*s)+1);
Wo2= Wo1;
Wo=[Wo1 0;0 Wo2];

freq = logspace(-5,5,100);

figure('Wi')
logplot(freq,funcWi(freq))

figure('Wo')
logplot(freq,funcWo(freq))

function wi = funcWi(x)
wi = ((x/(16*pi))+0.3)/((x/(64*pi))+1);
end 

function wo = funcWo(x)
wo = ((x/(16*pi))+0.3)/((x/(64*pi))+1);
end